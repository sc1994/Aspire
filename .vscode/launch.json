            "program": "${workspaceFolder}/examples/AuthAndAccount/bin/Debug/net6.0/AuthAndAccount.dll",
            "args": [],
            "cwd": "${workspaceFolder}/examples/AuthAndAccount",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
